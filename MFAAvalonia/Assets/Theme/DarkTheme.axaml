<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  添加深色主题的颜色  -->
    <SolidColorBrush
        x:Key="MouseOverRegionBrush"
        Opacity="0.75"
        Color="#303036" />
    <SolidColorBrush
        x:Key="WhiteBrush"
        Color="Black" />
    <!--  标签栏背景（半透明，让主题渐变透出来）  -->
    <SolidColorBrush x:Key="TabBarBackground" Color="#90101018" />
</ResourceDictionary>
