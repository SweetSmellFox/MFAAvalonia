<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  添加浅色主题的颜色  -->
    <SolidColorBrush
        x:Key="MouseOverRegionBrush"
        Opacity="0.75"
        Color="#eeeeee" />
    <SolidColorBrush
        x:Key="WhiteBrush"
        Color="White" />
    <!--  标签栏背景（半透明，让主题渐变透出来）  -->
    <SolidColorBrush x:Key="TabBarBackground" Color="#22B0B2B8" />
    <SolidColorBrush x:Key="TabBarHoverBackground" Color="#18000000" />
</ResourceDictionary>
